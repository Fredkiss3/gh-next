{
  "name": "gh-issues-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clean-dev": "rm -rf .next && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "next telemetry disable",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:build-deploy": "pnpm run pages:build && pnpm run pages:deploy",
    "pages:build-dev": "pnpm run pages:build && pnpm run pages:dev",
    "pages:deploy": "wrangler pages deploy .vercel/output/static",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "db:generate": "set -a; source .env.local; set +a && drizzle-kit generate:sqlite",
    "db:studio": "set -a; source .env.local; set +a && drizzle-kit studio --port 3003 --verbose",
    "db:migrate": "set -a; source .env.local; set +a && tsx ./migrate.ts",
    "cache:migrate": "set -a; source .env.local; set +a &&  drizzle-kit push:sqlite --config=./src/lib/kv/sqlite/config.ts"
  },
  "type": "module",
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@libsql/client": "^0.3.1",
    "@neondatabase/serverless": "^0.5.6",
    "@primer/octicons-react": "^19.4.0",
    "@t3-oss/env-nextjs": "^0.4.1",
    "bright": "^0.8.4",
    "cmdk": "^0.2.0",
    "dayjs": "^1.11.9",
    "drizzle-orm": "^0.28.0",
    "drizzle-zod": "^0.5.0",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.15",
    "isbot": "^3.6.13",
    "nanoid": "^4.0.2",
    "next": "13.4.17",
    "react": "18.2.0",
    "react-aria-components": "1.0.0-alpha.5",
    "react-dom": "18.2.0",
    "rehype-react": "^7.2.0",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "remark-github": "^11.2.4",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "server-only": "^0.0.1",
    "typescript": "5.1.3",
    "unified": "^10.1.2",
    "zod": "^3.21.4",
    "zod-form-data": "^2.0.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.3.1",
    "@cloudflare/workers-types": "^4.20230518.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "drizzle-kit": "^0.19.12",
    "pg": "^8.11.1",
    "postcss": "8.4.24",
    "postgres": "^3.3.5",
    "tailwindcss": "3.3.2",
    "tsx": "^3.12.7",
    "vercel": "^31.0.4",
    "wrangler": "^3.2.0"
  }
}
