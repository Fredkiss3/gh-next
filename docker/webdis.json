{
  "http_host": "0.0.0.0",
  "http_port": 7379,
  "redis_host": "redis",
  "redis_auth": "redis_password",
  "redis_port": 6379,

  "acl": [
    {
      "disabled": ["*"]
    },

    {
      "http_basic_auth": "user:password",
      "enabled": ["GET", "SET", "DEL", "SETEX"]
    }
  ],

  "verbosity": 4,
  "logfile": "/dev/stderr"
}
