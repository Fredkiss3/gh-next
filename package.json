{
  "name": "gh-issues-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo --experimental-https",
    "clean-dev": "rm -rf .next && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "next telemetry disable",
    "db:generate": "set -a; . ./.env.local; set +a && drizzle-kit generate:pg",
    "db:studio": "set -a; . ./.env.local; set +a && drizzle-kit studio --port 3003 --verbose",
    "db:migrate": "set -a; . ./.env.local; set +a && tsx ./migrate.ts",
    "db:migrate-docker": "tsx ./migrate.mjs",
    "db:drop-migration": "set -a; . ./.env.local; set +a && drizzle-kit drop",
    "format": "prettier . --write",
    "db:fetch-users": "set -a; . ./.env.local; set +a && tsx --tsconfig ./tsconfig.script.json ./scripts/fetchUsers.ts",
    "db:fetch-issues": "set -a; . ./.env.local; set +a && tsx --tsconfig ./tsconfig.script.json ./scripts/fetchIssues.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@mdx-js/mdx": "^3.0.0",
    "@neshca/cache-handler": "^0.6.2",
    "@neshca/json-replacer-reviver": "^1.1.0",
    "@primer/octicons-react": "^19.7.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toolbar": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@rsc-cache/next": "^1.1.1",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@tanstack/react-query": "^5.0.0",
    "bright": "^0.8.4",
    "client-only": "^0.0.1",
    "cmdk": "^0.2.0",
    "dayjs": "^1.11.9",
    "drizzle-orm": "^0.28.6",
    "drizzle-zod": "^0.5.1",
    "eslint": "8.48.0",
    "eslint-config-next": "14.0.0",
    "geist": "^1.0.0",
    "indent-textarea": "^4.0.0",
    "nanoid": "^4.0.2",
    "next": "14.1.0",
    "nextjs-toploader": "^1.6.4",
    "nprogress": "^0.2.0",
    "pg": "^8.11.3",
    "postgres": "^3.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-markdown": "^9.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^5.1.0",
    "remark": "^15.0.1",
    "remark-breaks": "^4.0.0",
    "remark-gemoji": "^8.0.0",
    "remark-gfm": "^4.0.0",
    "remark-github": "^12.0.0",
    "server-only": "^0.0.1",
    "sharp": "^0.32.5",
    "tailwind-merge": "^1.14.0",
    "text-field-edit": "^4.0.0",
    "vfile": "^6.0.1",
    "zod": "^3.22.2",
    "zod-form-data": "^2.0.1",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230821.0",
    "@faker-js/faker": "^8.0.2",
    "@tanstack/react-query-devtools": "^5.0.0",
    "@types/node": "20.5.7",
    "@types/nprogress": "^0.2.3",
    "@types/pg": "^8.10.2",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.15",
    "drizzle-kit": "^0.19.13",
    "postcss": "8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "3.3.3",
    "tsx": "^3.12.8",
    "typescript": "5.3.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "remark-github@12.0.0": "patches/remark-github@12.0.0.patch"
    }
  }
}
