{
  "name": "gh-issues-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clean-dev": "rm -rf .next && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "next telemetry disable",
    "db:generate": "set -a; . ./.env.local; set +a && drizzle-kit generate:pg",
