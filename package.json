    "db:migrate": "set -a; . ./.env.local; set +a && tsx ./migrate.ts",
    "format": "prettier . --write",
    "db:fetch-users": "set -a; . ./.env.local; set +a && tsx ./scripts/fetchUsers.ts",
    "db:fetch-issues": "set -a; . ./.env.local; set +a && tsx ./scripts/fetchIssues.ts"
  },
  "type": "module",
  "dependencies": {
